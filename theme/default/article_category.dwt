<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
<meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
<meta name="keywords" content="{$keywords}" />
<meta name="description" content="{$description}" />
<meta name="generator" content="DouPHP v1.1" />
<title>{$page_title}</title>
<link href="style.css" rel="stylesheet" type="text/css" />
<script type="text/javascript" src="images/jquery.min.js"></script>
<script type="text/javascript" src="images/global.js"></script>
</head>
<body>
<div id="wrapper">
 {include file="inc/header.tpl"}
 <div class="wrap mb">
   <div id="pageLeft">
    {include file="inc/article_tree.tpl"}
   </div>
   <div id="pageIn">
     {include file="inc/ur_here.tpl"}
     <div id="articleList">
       <!-- {foreach from=$article_list name=article_list item=article} -->
       <dl<!-- {if $smarty.foreach.article_list.last} --> class="last"<!-- {/if} -->>
         <div class="numDate">
           <em>{$article.click}</em>
           <p>{$article.add_time_short}</p>
         </div>
         <dt><a href="{$article.url}">{$article.title}</a></dt>
         <dd><!-- {if $article.image} --><p class="img"><img src="{$article.image}" height="42"></p><!-- {/if} --><p class="desc">{$article.description|truncate:96:"..."}</p></dd>
       </dl>
       <!-- {/foreach} -->
     </div>
     {include file="inc/pager.tpl"}
   </div>
   <div class="clear"></div>
 </div>
 {include file="inc/online_service.tpl"}
 {include file="inc/footer.tpl"} </div>
